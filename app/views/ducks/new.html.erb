<h1>New Duck</h1>

<%= form_for @duck do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :description %>
    <%= f.text_area :description%><br>

    <%= f.label "Owner" %>
    <%= f.collection_select :student_id, Student.all, :id, :name%><br>

    <%= f.submit %>
<% end %>